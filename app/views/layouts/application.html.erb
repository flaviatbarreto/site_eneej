<!DOCTYPE html>
<html>
<head>
  <title>SiteEneej</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="all">

	<div id="logo">
	</div>
	<div id="menu">
		<div id="links">
			<%= render "layouts/menu" %>

			<%= render "layouts/user"%>
		</div>
	</div>

	<div id="main">
		<% if !flash.empty? %>
			<% flash.each do |value,key| %>
				<div id="<%= value %>"><%= key %></div>
			<% end %>
		<% end %>


	<%= yield %>
	</div>
<div>
</body>
</html>
