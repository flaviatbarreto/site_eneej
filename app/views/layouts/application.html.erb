<!DOCTYPE html>
<html>
<head>
  <title>SiteEneej</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<% if !flash.empty? %>
	<% flash.each do |value,key| %>
		<div id="<%= value %>"><%= key %></div>
	<% end %>
<% end %>

<%= yield %>

</body>
</html>
